ark={unSaveItem} localItems = {diy} />} />\n                <Route path='bookmark' element={<Bookmark item = {diy} bookmarked={saveItem} unBookmark={unSaveItem} localItems = {diy}/>} />\n              </Route>\n            </Routes>\n        </BrowserRouter>\n    </>\n  );\n}\n\nexport default App;\n","D:\\ReactJS\\homemadearthouse\\src\\Navbar.js",["75","76"],[],"import React from \"react\";\r\nimport './navbar.css';\r\nimport { BrowserRouter, Route, Link } from \"react-router-dom\";\r\nimport { useState } from \"react\";\r\nconst Navbar = () => {\r\n    const [showMenu, setShowMenu] = useState(false);\r\n\r\n    const toggleMenu = () => {\r\n        setShowMenu((prevShowMenu) => !prevShowMenu);\r\n    };\r\n    return (\r\n        <>\r\n            <header>\r\n                <div className=\"container\">\r\n                    <div className=\"row\">\r\n                        <div className=\"col col-1\" >\r\n                            <Link className=\"logo_icon\"  to=\"/\"></Link>\r\n                            <div className=\"logo_name\" to=\"/index\">HomemadeArtHouse</div>\r\n                        </div>\r\n                        <div className={`col col-2 ${showMenu ? \"active\" : \"\"}`}>\r\n                            <div className=\"nav\">\r\n                                <ul>\r\n                                    <li><Link to=\"/\">Home</Link></li>\r\n                                    <li><Link to=\"/about\">About</Link></li>\r\n                                    <li><Link to=\"/explore\">Explore</Link></li>\r\n                                </ul>\r\n                            </div>\r\n                            <div className=\"buttons\">\r\n                                <Link to=\"/bookmark\">Saved</Link>\r\n                            </div>\r\n                        </div>\r\n                        <div className={`menu_btn ${showMenu ? \"active\" : \"\"}`} onClick={toggleMenu} >\r\n                        <i class=\"fa-solid fa-